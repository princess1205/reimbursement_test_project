<%= render 'employees/bootstrap_style' %>
<h1>Employee Claims bill list</h1><%= button_to "Add new expense", "bils/create" ,method: "get" %>
<table class="table">
  <tr>
    <th>Employee</th>
    <th>claim of expense</th>
    <th>Amount</th>
  </tr>
  <% @bils_list.each do |obj| %>
    <tr>
        <td><%= obj.first_name%></td>
        <td><%=obj.claim_for%></td>
        <td><%=obj.amount%></td><td>
    </tr>
  <% end %>
</table>
